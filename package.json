{
  "name": "koa-github-hook-handler",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/hook-handler.mjs",
  "description": "handle github hook requests",
  "keywords": [
    "github",
    "hook"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "docs": "documentation readme {{module}} --section=API",
    "lint": "documentation lint {{module}}",
    "posttest": "markdown-doctest",
    "test": "ava"
  },
  "dependencies": {
    "raw-body": "^2.4.0"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "c8": "^3.5.0",
    "documentation": "^10.0.0",
    "esm": "^3.2.22",
    "got": "^9.6.0",
    "koa": "^2.7.0",
    "koa-better-router": "^2.1.1",
    "markdown-doctest": "^0.9.1",
    "semantic-release": "^15.13.9",
    "x-hub-signature": "^1.2.0"
  },
  "engines": {
    "node": ">=10.15.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/koa-github-hook-handler.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/koa-github-hook-handler/issues"
  },
  "homepage": "https://github.com/arlac77/koa-github-hook-handler#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "extensions": [
      "js",
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
